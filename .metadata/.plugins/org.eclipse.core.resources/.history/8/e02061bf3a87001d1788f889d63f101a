package chapter01;

public class Example01 {

	public static void main(String[] args) {
		// 데이터 타입
		//
		// 정수형 타입
		// byte (1 byte), short (2 byte), int (4 byte), long (8 byte)
		byte b1 = 0;
		byte b2 = 127;
		byte b3 = -128;
		
		short s1 = 128;
		short s2 = 32767;
		short s3 = -32768;
		
	}

}
