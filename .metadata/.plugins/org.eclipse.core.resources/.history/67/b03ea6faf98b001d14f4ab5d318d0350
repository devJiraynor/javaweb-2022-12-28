package chapter02;

// 게시물 작성
// 게시물 목록
// 게시물 상세 보기
// 게시물 수정
class Board {
	// 게시물 구분자
	int id;
	// 제목
	String title;
	// 작성자
	String writer;
	// 작성 날짜 및 시간
	String writeDate;
	// 내용
	String contents;
	// 좋아요
	int like;
}

public class Sample01 {

	public static void main(String[] args) {
		// 프로그램은 사용자가 종료를 하기전까지 계속 실행되어야 함
		// 사용자로 부터 입력을 받아 게시물 작성, 게시물 목록 기능을 선택할 수 있음
	}

}








